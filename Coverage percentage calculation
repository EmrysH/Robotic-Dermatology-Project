import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt
import numpy as np
Shots = pd.read_excel(io="E:\Files\ROMI-Lab Works\ROMI_CV\Resource\Data\Robot&Human.xlsx", usecols="C,R")

AVG_Distance = pd.read_excel(io="E:\Files\ROMI-Lab Works\ROMI_CV\Resource\Data\Robot&Human.xlsx", usecols="D,S")

Min_Distance = pd.read_excel(io="E:\Files\ROMI-Lab Works\ROMI_CV\Resource\Data\Robot&Human.xlsx", usecols="E,T")

Max_Distance = pd.read_excel(io="E:\Files\ROMI-Lab Works\ROMI_CV\Resource\Data\Robot&Human.xlsx", usecols="F,U")

AVG_Angle = pd.read_excel(io="E:\Files\ROMI-Lab Works\ROMI_CV\Resource\Data\Robot&Human.xlsx", usecols="H,W")

Min_Angle = pd.read_excel(io="E:\Files\ROMI-Lab Works\ROMI_CV\Resource\Data\Robot&Human.xlsx", usecols="I,X")

Max_Angle = pd.read_excel(io="E:\Files\ROMI-Lab Works\ROMI_CV\Resource\Data\Robot&Human.xlsx", usecols="J,Y")

plt.subplot(3, 3, 1)
sns.histplot(data = AVG_Distance,stat = "probability", kde = True)
plt.xlabel("Average_Distance")

plt.subplot(3, 3, 2)
sns.histplot(data = Shots,stat = "probability", kde = True)
plt.xlabel("Number of Shots")

plt.subplot(3, 3, 3)
sns.histplot(data = Min_Distance,stat = "probability", kde = True)
plt.xlabel("Min_Distance")

plt.subplot(3, 3, 4)
sns.histplot(data = Max_Distance,stat = "probability", kde = True)
plt.xlabel("Max_Distance")

plt.subplot(3, 3, 5)
sns.histplot(data = AVG_Angle,stat = "probability", kde = True)
plt.xlabel("Average_Angle")

plt.subplot(3, 3, 6)
sns.histplot(data = Min_Angle,stat = "probability", kde = True)
plt.xlabel("Min_Angle")

plt.subplot(3, 3, 7)
sns.histplot(data = Max_Angle,stat = "probability", kde = True)
plt.xlabel("Max_Angle")

plt.show()
